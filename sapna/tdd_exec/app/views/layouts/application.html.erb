<!DOCTYPE html>
<html>
<head>
  <title>TddExec</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div>
  <%= flash[:notice] if flash[:notice].present? %>
  <%= flash[:error] if flash[:error].present? %>
</div>
<%= yield %>

</body>
</html>
